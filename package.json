{
  "name": "weather-test2",
  "version": "2.0.0",
  "description": "Sample weather SPA",
  "main": "app.js",
  "scripts": {
    "start": "gulp serve",
    "test": "karma start",
    "postinstall": "node node_modules/node-sass/scripts/install.js && yarn add --dev node-sass --force --ignore-scripts && node node_modules/phantomjs-prebuilt/install.js && node_modules/jspm/jspm.js registry create bower jspm-bower-endpoint && node_modules/jspm/jspm.js install",
    "protractor": "gulp protractor"
  },
  "keywords": [
    "weather",
    "angularjs",
    "spa",
    "es6",
    "jspm",
    "gulp"
  ],
  "author": "Danilo Cestari",
  "license": "ISC",
  "jspm": {
    "name": "app",
    "main": "app.js",
    "dependencies": {
      "angular": "github:angular/bower-angular@^1.5.8",
      "angular-mocks": "github:angular/bower-angular-mocks@^1.5.8",
      "angular-resource": "github:angular/bower-angular-resource@^1.5.8",
      "angular-ui-router": "github:angular-ui/angular-ui-router-bower@^0.3.1",
      "babel-core": "npm:babel-core@^6.22.1",
      "bitters": "bower:bitters@^1.4.0",
      "countries-list": "npm:countries-list@^1.2.0",
      "jquery": "npm:jquery@^3.1.1",
      "json": "github:systemjs/plugin-json@^0.2.0",
      "moment": "npm:moment@^2.15.1",
      "normalize-css": "bower:normalize-css@^4.2.0"
    },
    "devDependencies": {
      "mdi": "npm:mdi@^1.8.36",
      "plugin-babel": "npm:systemjs-plugin-babel@^0.0.15"
    },
    "peerDependencies": {
      "assert": "npm:jspm-nodelibs-assert@^0.2.0",
      "buffer": "npm:jspm-nodelibs-buffer@^0.2.0",
      "child_process": "npm:jspm-nodelibs-child_process@^0.2.0",
      "constants": "npm:jspm-nodelibs-constants@^0.2.0",
      "crypto": "npm:jspm-nodelibs-crypto@^0.2.0",
      "events": "npm:jspm-nodelibs-events@^0.2.0",
      "fs": "npm:jspm-nodelibs-fs@^0.2.0",
      "module": "npm:jspm-nodelibs-module@^0.2.0",
      "os": "npm:jspm-nodelibs-os@^0.2.0",
      "path": "npm:jspm-nodelibs-path@^0.2.0",
      "process": "npm:jspm-nodelibs-process@^0.2.0",
      "stream": "npm:jspm-nodelibs-stream@^0.2.0",
      "string_decoder": "npm:jspm-nodelibs-string_decoder@^0.2.0",
      "util": "npm:jspm-nodelibs-util@^0.2.0",
      "vm": "npm:jspm-nodelibs-vm@^0.2.0"
    },
    "overrides": {
      "github:angular-ui/angular-ui-router-bower@0.3.2": {
        "dependencies": {
          "angular": "*"
        },
        "meta": {
          "release/angular-ui-router.js.js": {
            "deps": [
              "angular"
            ],
            "format": "global"
          }
        }
      },
      "github:angular/bower-angular-mocks@1.6.1": {
        "main": "angular-mocks",
        "registry": "jspm",
        "dependencies": {
          "angular": "github:angular/bower-angular@^1.3.0"
        },
        "meta": {
          "angular-mocks.js": {
            "deps": [
              "angular"
            ],
            "format": "global"
          }
        }
      },
      "github:angular/bower-angular-resource@1.6.1": {
        "main": "angular-resource",
        "registry": "jspm",
        "dependencies": {
          "angular": "^1.4.8"
        },
        "meta": {
          "angular-resource.js": {
            "deps": [
              "angular"
            ],
            "format": "global"
          }
        }
      },
      "github:angular/bower-angular@1.6.1": {
        "main": "angular",
        "meta": {
          "angular.js": {
            "exports": "angular",
            "format": "global"
          }
        }
      },
      "npm:debug@2.6.0": {
        "main": "src/browser.js",
        "jspmNodeConversion": false,
        "format": "cjs",
        "map": {
          "./src/browser.js": {
            "node": "./src/node.js"
          },
          "./node.js": {
            "browser": "./src/browser.js"
          },
          "fs": "@node/fs",
          "net": "@node/net",
          "tty": "@node/tty",
          "util": "@node/util"
        }
      },
      "npm:inherits@2.0.3": {
        "ignore": [
          "test.js"
        ]
      },
      "npm:jquery@3.1.1": {
        "format": "amd"
      },
      "npm:lodash@4.17.4": {
        "map": {
          "buffer": "@empty",
          "process": "@empty"
        }
      },
      "npm:moment@2.17.1": {
        "jspmNodeConversion": false
      },
      "npm:ms@0.7.2": {
        "jspmNodeConversion": false,
        "format": "cjs"
      }
    }
  },
  "devDependencies": {
    "autoprefixer": "^6.7.0",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-register": "^6.16.3",
    "browser-sync": "^2.17.0",
    "connect-history-api-fallback": "^1.3.0",
    "del": "^2.2.2",
    "gulp": "^3.9.1",
    "gulp-angular-protractor": "^0.3.0",
    "gulp-angular-templatecache": "^2.0.0",
    "gulp-cached": "^1.1.0",
    "gulp-concat": "^2.6.0",
    "gulp-cssnano": "^2.1.2",
    "gulp-html-replace": "^1.6.1",
    "gulp-htmlmin": "^3.0.0",
    "gulp-ng-annotate": "^2.0.0",
    "gulp-postcss": "^6.2.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^3.1.0",
    "gulp-svgmin": "^1.2.3",
    "gulp-uglify": "^2.0.0",
    "gulp-util": "^3.0.7",
    "jasmine-core": "^2.5.2",
    "jshint": "^2.9.3",
    "jspm": "0.17.0-beta.32",
    "jspm-bower-endpoint": "^0.3.2",
    "karma": "^1.4.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-jspm": "^2.2.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-spec-reporter": "0.0.26",
    "node-bourbon": "^4.2.8",
    "node-neat": "^1.7.2",
    "node-sass": "^4.3.0",
    "path": "^0.12.7",
    "phantomjs-prebuilt": "^2.1.12",
    "postcss-assets": "^4.1.0",
    "postcss-copy": "^6.2.1",
    "postcss-import": "^9.1.0",
    "protractor": "^5.0.0",
    "require-dir": "^0.3.0",
    "run-sequence": "^1.2.2"
  }
}
